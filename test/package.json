{
  "name": "bedrock-tokenizer-test",
  "version": "0.0.1-0",
  "description": "Bedrock Tokenizer Tests",
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "test:debug": "node --preserve-symlinks test.js test --log-level debug",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcov npm test",
    "coverage-report": "nyc report"
  },
  "author": "",
  "dependencies": {
    "bedrock": "^4.4.0",
    "bedrock-app-identity": "^1.0.0",
    "bedrock-did-context": "^2.0.0",
    "bedrock-did-io": "^6.0.2",
    "bedrock-express": "^6.2.0",
    "bedrock-https-agent": "^2.1.0",
    "bedrock-jsonld-document-loader": "^1.1.0",
    "bedrock-kms": "^8.0.0",
    "bedrock-kms-http": "^11.0.0",
    "bedrock-ledger-context": "^21.0.1",
    "bedrock-meter": "^1.2.0",
    "bedrock-meter-http": "^4.0.0",
    "bedrock-meter-usage-reporter": "^5.0.1",
    "bedrock-mongodb": "^8.4.1",
    "bedrock-package-manager": "^1.1.0",
    "bedrock-security-context": "^5.0.0",
    "bedrock-server": "^3.0.0",
    "bedrock-ssm-mongodb": "^7.0.1",
    "bedrock-test": "^6.0.0",
    "bedrock-tokenizer": "file://..",
    "bedrock-validation": "^5.2.2",
    "bedrock-veres-one-context": "^12.0.0",
    "bedrock-zcap-storage": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint-config-digitalbazaar": "^2.8.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-tokenizer/**"
    ],
    "exclude": [
      "node_modules/bedrock-tokenizer/node_modules/**"
    ]
  }
}
